{
  "name": "flix",
  "version": "1.0.0",
  "description": "Flix - X402 Video Platform with Instant Creator Monetization on Solana",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --watch server --ext ts,json --exec ts-node --project tsconfig.server.json server/index.ts",
    "dev:client": "vite",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "start": "node dist/server/index.js",
    "type-check": "tsc --noEmit",
    "seed": "ts-node scripts/seed-videos.ts"
  },
  "keywords": [
    "x402",
    "video",
    "platform",
    "solana",
    "usdc",
    "micropayments"
  ],
  "author": "",
  "license": "MIT",
